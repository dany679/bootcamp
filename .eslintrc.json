{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "only-warn"
    ],
    "rules": {
        "camelcase":"off",
         "no-const-assign": "warn",
        "no-this-before-super": "warn"
    }
}
